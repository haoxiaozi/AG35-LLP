#!/bin/sh
# Expected to be called as: depmodwrapper -a KERNEL_VERSION
if [ "$1" != "-a" -o "$2" != "-b" ]; then
    echo "Usage: depmodwrapper -a -b rootfs KERNEL_VERSION" >&2
    exit 1
fi
if [ ! -r /home/quectel/Alessa_Linux_193/Qualcomm/MDM9x28/OpenLinux/LE2.0.1/release/AG35CEVAR05A03T4G_OCPU/apps_proc/poky/build/tmp-glibc/sysroots/mdm9607/pkgdata/kernel-depmod/kernel-abiversion ]; then
    echo "Unable to read: /home/quectel/Alessa_Linux_193/Qualcomm/MDM9x28/OpenLinux/LE2.0.1/release/AG35CEVAR05A03T4G_OCPU/apps_proc/poky/build/tmp-glibc/sysroots/mdm9607/pkgdata/kernel-depmod/kernel-abiversion" >&2
else
    kernelabi=$(cat /home/quectel/Alessa_Linux_193/Qualcomm/MDM9x28/OpenLinux/LE2.0.1/release/AG35CEVAR05A03T4G_OCPU/apps_proc/poky/build/tmp-glibc/sysroots/mdm9607/pkgdata/kernel-depmod/kernel-abiversion)
    if [ "$kernelabi" != "$4" ]; then
        echo "Error: Kernel version $4 does not match kernel-abiversion ($kernelabi)" >&2
        exit 1
    fi
fi

if [ ! -r /home/quectel/Alessa_Linux_193/Qualcomm/MDM9x28/OpenLinux/LE2.0.1/release/AG35CEVAR05A03T4G_OCPU/apps_proc/poky/build/tmp-glibc/sysroots/mdm9607/pkgdata/kernel-depmod/System.map-$4 ]; then
    echo "Unable to read: /home/quectel/Alessa_Linux_193/Qualcomm/MDM9x28/OpenLinux/LE2.0.1/release/AG35CEVAR05A03T4G_OCPU/apps_proc/poky/build/tmp-glibc/sysroots/mdm9607/pkgdata/kernel-depmod/System.map-$4" >&2
    exec env depmod "$1" "$2" "$3" "$4"
else
    exec env depmod "$1" "$2" "$3" -F "/home/quectel/Alessa_Linux_193/Qualcomm/MDM9x28/OpenLinux/LE2.0.1/release/AG35CEVAR05A03T4G_OCPU/apps_proc/poky/build/tmp-glibc/sysroots/mdm9607/pkgdata/kernel-depmod/System.map-$4" "$4"
fi
